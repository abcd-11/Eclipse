package hackerrank;
import java.util.Scanner;

public class pompom_pattern {
	static int major;
		public static void main(String args[])
		{
			Scanner sc=new Scanner(System.in);
			int n=Integer.parseInt(sc.nextLine());
			print_hexagon(n);
		}
		public static void print_hexagon(int n)
		{  major=n;
		   for(int i=1;i<=n;i++)
		   {
			   int j=n-i;
			   while(j>=0)
			   {
				   System.out.printf(" ");
				   j--;
			   }
			   
		   }
			
		}
}
